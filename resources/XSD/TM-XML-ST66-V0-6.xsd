	<!-- *====================================================================* -->
	<!-- *                                                                    * -->
	<!-- * XML Schema of TM-XML Data Model for Trade Marks                    * -->
	<!-- *                                                                    * -->
	<!-- ********************************************************************** -->

	<xs:annotation>
		<xs:documentation>TM-XML ST66 version, dated 27th May 2015</xs:documentation>
	</xs:annotation>

	<!-- ====================================================================== -->
	<!-- Include of external standard types                                     -->
	<!-- ====================================================================== -->
	<xs:include schemaLocation="external/ISOCountryCodeType-V2011.xsd"/>
	<xs:include schemaLocation="external/ISOLanguageCodeType-V2002.xsd"/>
